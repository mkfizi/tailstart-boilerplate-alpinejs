{
	"name": "tailstart-kit-boilerplate",
	"version": "0.2.1",
	"description": "Tailstart Kit boilerplate starter kit in HTML and AlpineJS.",
	"keywords": [
		"Tailwindcss",
		"Starter Kit",
		"Boilerplate"
	],
	"author": "hi@mkfizi.dev",
	"license": "MIT",
	"scripts": {
		"build": "npx tailwindcss -i ./src/css/tailwind.css -o ./src/css/app.css ",
		"watch": "npx tailwindcss -i ./src/css/tailwind.css -o ./src/css/app.css --watch"
	},
	"devDependencies": {
		"tailwindcss": "^3.3.3"
	}
}